<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net46</TargetFramework>
  </PropertyGroup>

  <PropertyGroup>
    <OutputPath>..\Bin\v4</OutputPath>
  </PropertyGroup>

  <ItemGroup>
    <Compile Include="..\Shared\Benchmark.cs" Link="Benchmark.cs" />
    <Compile Include="..\Shared\ImportHelper.cs" Link="ImportHelper.cs" />
    <Compile Include="..\Shared\ScriptRegistrator.cs" Link="ScriptRegistrator.cs" />
    <Compile Include="..\Shared\SharedProgram.cs" Link="SharedProgram.cs" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="DryIoc.dll" Version="4.8.1" />
