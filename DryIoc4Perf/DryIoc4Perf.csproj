    <PackageReference Include="Zyan" Version="2.15.0" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="Metadata">
      <HintPath>..\References\Metadata.dll</HintPath>
    </Reference>
    <Reference Include="Interfaces">
      <HintPath>..\References\Interfaces.dll</HintPath>
    </Reference>
    <Reference Include="Scripts">
      <HintPath>..\References\Scripts.dll</HintPath>
    </Reference>
    <Reference Include="Services">
      <HintPath>..\References\Services.dll</HintPath>
    </Reference>
  </ItemGroup>

</Project>
